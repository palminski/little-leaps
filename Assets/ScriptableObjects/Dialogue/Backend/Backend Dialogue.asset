    - option: Max Attunement
      nextDialogue: {fileID: 0}
      optionEvent: {fileID: 0}
      triggerEvent: {fileID: 11400000, guid: 51eba1cc9c628604ea45bafa5a32f961, type: 2}
      sideText: "Set RABIT Pilot attunement to max. \nThis will result in a difficult
        climb."
    - option: Do Not Attune
